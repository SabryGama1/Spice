@model int
<div class="btn-group" role="group">
    <a asp-action="Details" class="btn btn-outline-success " asp-route-id="@Model">
        <i class="fa-regular fa-circle-info"></i>
    </a>
    <a asp-action="Edit" class="btn btn-outline-primary " asp-route-id="@Model">
        <i class="fa-regular fa-pen-to-square"></i>
    </a>
    <a asp-action="Delete" class="btn btn-outline-danger " asp-route-id="@Model">
        <i class="fa-solid fa-trash"></i>
    </a>

</div>
